#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:1076
#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:1094
msgid "RMB"
msgstr "Botón derecho del ratón"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:1084
msgid "'Favorite Layout' Move Mode Activator"
msgstr "Activador del modo de movimiento 'Distribución favorita'"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:1102
