msgstr "Mover la ventana al centro"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:595
#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:978
msgid "Edge Tiling"
msgstr "Disposición en mosaico en los bordes de la pantalla"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:613
msgid "Tile to top"
msgstr "Disponer en mosaico en el borde superior"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:621
msgid "Tile to bottom"
msgstr "Disponer en mosaico en el borde inferior"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:629
msgid "Tile to left"
msgstr "Disponer en mosaico en el borde izquierdo"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:637
msgid "Tile to right"
msgstr "Disponer en mosaico en el borde derecho"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:650
msgid "Corner Tiling"
msgstr "Disposición en mosaico en las esquinas de la pantalla"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:667
msgid "Tile to top-left"
msgstr "Disponer en mosaico en la esquina superior izquierda"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:675
msgid "Tile to top-right"
msgstr "Disponer en mosaico en la esquina superior derecha"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:683
msgid "Tile to bottom-left"
msgstr "Disponer en mosaico en la esquina inferior izquierda"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:691
msgid "Tile to bottom-right"
msgstr "Disponer en mosaico en la esquina inferior derecha"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:710
#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:775
msgid "Layouts"
msgstr "Distribuciones"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:750
msgid "Panel Indicator"
msgstr "Mostrar indicador en el panel"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:762
msgid "Search for a Layout"
msgstr "Buscar una distribución"

#: tiling-assistant@leleat-on-github/src/ui/prefs.ui:763
msgid "Open a popup listing all the available layouts"
msgstr ""
"Abre una ventana emergente con una lista de todas las distribuciones "
