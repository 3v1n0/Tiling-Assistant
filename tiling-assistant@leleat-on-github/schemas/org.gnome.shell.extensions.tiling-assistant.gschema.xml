<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<enum id="org.gnome.shell.extensions.tiling-assistant.restore-window-size">
		<value value="0" nick="Grab Start"/>
		<value value="1" nick="Grab End"/>
	</enum>

	<enum id="org.gnome.shell.extensions.tiling-assistant.dynamic-keybindings">
		<value value="0" nick="Disabled"/>
		<value value="1" nick="Focus"/>
		<value value="2" nick="Tiling State"/>
		<value value="3" nick="Tiling State (Windows)"/>
	</enum>

	<enum id="org.gnome.shell.extensions.tiling-assistant.secondary-tiling-preview-activators">
		<value value="0" nick="Ctrl"/>
		<value value="1" nick="Alt"/>
		<value value="2" nick="RMB"/>
	</enum>

	<schema id="org.gnome.shell.extensions.tiling-assistant" path="/org/gnome/shell/extensions/tiling-assistant/">
		<key name="enable-tiling-popup" type="b">
			<default>true</default>
		</key>
		<key name="enable-raise-tile-group" type="b">
			<default>true</default>
		</key>
		<key name="dynamic-keybinding-behaviour" enum="org.gnome.shell.extensions.tiling-assistant.dynamic-keybindings">
			<default>"Disabled"</default>
		</key>
		<key name="window-gap" type="i">
			<default>0</default>
		</key>
		<key name="maximize-with-gap" type="b">
			<default>false</default>
		</key>
		<key name="restore-window-size-on" enum="org.gnome.shell.extensions.tiling-assistant.restore-window-size">
			<default>"Grab Start"</default>
		</key>
		<key name="enable-hold-maximize-inverse-landscape" type="b">
			<default>false</default>
		</key>
		<key name="enable-hold-maximize-inverse-portrait" type="b">
			<default>false</default>
		</key>
		<key name="toggle-tiling-popup" type="as">
			<default>[]</default>
		</key>
		<key name="tile-edit-mode" type="as">
			<default>[]</default>
		</key>
		<key name="auto-tile" type="as">
			<default>[]</default>
		</key>
		<key name="tile-maximize" type="as">
			<default><![CDATA[['<Super>KP_5']]]></default>
		</key>
		<key name="tile-top-half" type="as">
			<default><![CDATA[['<Super>KP_8']]]></default>
		</key>
		<key name="tile-bottom-half" type="as">
			<default><![CDATA[['<Super>KP_2']]]></default>
		</key>
		<key name="tile-left-half" type="as">
			<default><![CDATA[['<Super>KP_4']]]></default>
		</key>
		<key name="tile-right-half" type="as">
			<default><![CDATA[['<Super>KP_6']]]></default>
		</key>
		<key name="tile-topleft-quarter" type="as">
			<default><![CDATA[['<Super>KP_7']]]></default>
		</key>
		<key name="tile-topright-quarter" type="as">
			<default><![CDATA[['<Super>KP_9']]]></default>
		</key>
		<key name="tile-bottomleft-quarter" type="as">
			<default><![CDATA[['<Super>KP_1']]]></default>
		</key>
		<key name="tile-bottomright-quarter" type="as">
			<default><![CDATA[['<Super>KP_3']]]></default>
		</key>
		<key name="tiling-popup-current-workspace-only" type="b">
			<default>true</default>
		</key>
		<key name="tile-editing-mode-color" type="s">
	  		<default>"rgb(27, 106, 203)"</default>
		</key>
		<key name="secondary-tiling-preview-activator" enum="org.gnome.shell.extensions.tiling-assistant.secondary-tiling-preview-activators">
			<default>"Ctrl"</default>
		</key>
		<key name="default-to-secondary-tiling-preview" type="b">
			<default>false</default>
		</key>
		<key name="enable-tile-animations" type="b">
			<default>true</default>
		</key>
		<key name="enable-untile-animations" type="b">
			<default>true</default>
		</key>
		<key name="toggle-maximize-tophalf-timer" type="i">
			<default>600</default>
		</key>
		<key name="vertical-preview-area" type="i">
			<default>15</default>
		</key>
		<key name="horizontal-preview-area" type="i">
			<default>15</default>
		</key>
		<key name="debugging-show-tiled-rects" type="as">
			<default>[]</default>
		</key>
		<key name="debugging-free-rects" type="as">
			<default>[]</default>
		</key>
	</schema>
</schemalist>
